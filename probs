			if (value < 0)
				return false;
			int root(round(sqrt(value)));
			return value == root * root; }
		)
		})


bool is_perfect_square(int n) {
	if (n < 0)
		return false;
	int root(round(sqrt(n)));
	return n == root * root;
}
*/

int main()
{
	vector<int> vec;

	for (int i = 1; i <= 9; i++)
		vec.push_back(i);

	for (int i = 0; i < 9; i++) {
		int current_value = vec[i];
		if ([current_value](int value) { int root(round(sqrt(value))); if (value > 0 && value == root * root) return true; })
			std::cout << current_value;
	}
}

